<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_consultants">

    <fragment
        android:id="@+id/navigation_consultants"
        android:name="com.consult.me.app.ui.fragments.UsersConsultantsFragment"
        android:label="@string/menu_consultants"
        tools:layout="@layout/fragment_users" />

    <fragment
        android:id="@+id/navigation_clients"
        android:name="com.consult.me.app.ui.fragments.UsersClientsFragment"
        android:label="@string/menu_clients"
        tools:layout="@layout/fragment_users" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.consult.me.app.ui.fragments.ProfileFragment"
        android:label="@string/str_more_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_categories"
        android:name="com.consult.me.app.ui.fragments.CategoriesFragment"
        android:label="@string/str_more_categories"
        tools:layout="@layout/fragment_categories" />

    <fragment
        android:id="@+id/navigation_about"
        android:name="com.consult.me.app.ui.fragments.AboutFragment"
        android:label="@string/str_more_about"
        tools:layout="@layout/fragment_about" />
</navigation>